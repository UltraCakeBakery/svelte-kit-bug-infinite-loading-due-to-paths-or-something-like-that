		"@unocss/transformer-variant-group": "^0.44.0",
		"dayjs": "^1.11.3",
		"svelte": "^3.49.0",
		"svelte-check": "^2.8.0",
		"typescript": "^4.7.4",
		"unocss": "^0.44.0",
		"vite": "^2.9.14"
